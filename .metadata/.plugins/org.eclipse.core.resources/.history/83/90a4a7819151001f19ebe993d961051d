package com.pokemon.solgario.week1introduction.homework;

import org.springframework.stereotype.Component;

@Component
public class Strawberry implements Frosting, Syrup{
	
	final private Frosting frost;
	final private Syrup syrup;
	
	Strawberry(Frosting frost, Syrup syrup) {
		this.frost = frost;
		this.syrup = syrup;
	}
	
	@Override
	public String getFrostingType() {
		// TODO Auto-generated method stub
		return "Strawberry Frosting";
	}
	
	@Override
	public String getSyrupType() {
		// TODO Auto-generated method stub
		return "Starberry Syrup";
	}
	
}


